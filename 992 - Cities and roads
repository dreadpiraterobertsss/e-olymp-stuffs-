#include <bits/stdc++.h>
using namespace std;

int main() {
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);

    int n;
    cin >> n;
    int ans = 0;
    
    for(int i = 0; i  <n ; i++){
        for(int j = 0; j < n ; j++){
            int m;
            cin >> m;
            if(m == 1 && i < j){
                ans ++;
            }
        }
    }
    
    

    cout << ans ;

}
