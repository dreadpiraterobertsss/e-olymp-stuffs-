#include <bits/stdc++.h>
using namespace std;

int main() {
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);

    int n;
    cin >> n;
    stack<int> arr;
    for (int i = 0; i < n; i++){
        int f;
        cin >> f;
        if(f == 1){
            int s;
            cin >> s;
            arr.push(s);
        }
        else{
            cout << arr.top() << "\n";
            arr.pop();
        }
    }
    return 0;
}
